<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\..\..\configureawait.props" />
  <Import Project="..\..\..\..\modules.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>X.Abp.AuditLogging.Application.Contracts</AssemblyName>
    <PackageId>X.Abp.AuditLogging.Application.Contracts</PackageId>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <RootNamespace />
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="X\Abp\AuditLogging\Localization\Resources\*.json" />
    <Content Remove="X\Abp\AuditLogging\Localization\Resources\*.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AuditLogging.Domain.Shared"
      VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Ddd.Application.Contracts"
      VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Authorization.Abstractions"
      VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Features" VersionOverride="$(AbpPackageVersion)" />
  </ItemGroup>

</Project>
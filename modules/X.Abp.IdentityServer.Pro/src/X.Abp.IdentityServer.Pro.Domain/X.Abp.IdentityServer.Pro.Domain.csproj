<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\..\..\configureawait.props" />
  <Import Project="..\..\..\..\modules.props" />

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>X.Abp.IdentityServer.Pro.Domain</AssemblyName>
    <PackageId>X.Abp.IdentityServer.Pro.Domain</PackageId>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace />
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\X.Abp.Identity.Pro\src\X.Abp.Identity.Pro.Domain\X.Abp.Identity.Pro.Domain.csproj" />
    <ProjectReference Include="..\X.Abp.IdentityServer.Pro.Domain.Shared\X.Abp.IdentityServer.Pro.Domain.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.IdentityServer.Domain.Shared" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Identity.Domain" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.AutoMapper" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Security" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Caching" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.Validation" VersionOverride="$(AbpPackageVersion)" />
    <PackageReference Include="Volo.Abp.BackgroundWorkers" VersionOverride="$(AbpPackageVersion)" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="IdentityServer4" />
    <PackageReference Include="IdentityServer4.AspNetIdentity" />
  </ItemGroup>

</Project>